# InfluxDB Configuration
# Copy this file to .env and replace with your actual values

# InfluxDB API Token - REQUIRED
# Get this from your InfluxDB instance under Data > Tokens
INFLUX_TOKEN=your_influxdb_token_here

# InfluxDB URL - default for local development
INFLUX_URL=http://127.0.0.1:8181

# InfluxDB Bucket name
INFLUX_BUCKET=ThetaData

# InfluxDB Organization (if needed)
# INFLUX_ORG=your_org_here
